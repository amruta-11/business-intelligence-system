    <DTS:Executable
      DTS:refId="Package\Load factSalesActual"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{7FAACE65-EC39-4297-BE13-1997BD748D54}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Load factSalesActual"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="1">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C6FF1FD3-3CE7-4B89-9E4C-1AE8E11CE01E}"
          SQLTask:SqlStatementSource="&#xA;IF EXISTS (SELECT TABLE_NAME FROM INFORMATION_SCHEMA.TABLES WHERE TABLE_SCHEMA = 'dbo' AND TABLE_NAME = 'factSalesActual')&#xA;BEGIN&#xA;&#x9;-- ====================================&#xA;&#x9;-- Load factSalesActual table&#xA;&#x9;-- ====================================&#xA;&#xA;&#x9;INSERT INTO dbo.factSalesActual&#xA;&#x9;(&#xA;&#x9;dimProductID,&#xA;&#x9;dimStoreID,&#xA;&#x9;dimResellerID,&#xA;&#x9;dimCustomerID,&#xA;&#x9;dimChannelID,&#xA;&#x9;dimSalesDateID,&#xA;&#x9;dimLocationID,&#xA;&#x9;factSalesHeaderID,&#xA;&#x9;factSalesDetailID,&#xA;&#x9;factSaleAmount,&#xA;&#x9;factSaleQuantity,&#xA;&#x9;factSaleUnitPrice,&#xA;&#x9;factSaleExtendedCost,&#xA;&#x9;factSaleTotalProfit&#xA;&#x9;)&#xA;SELECT&#xA;dimProduct.dimProductID AS dimProductID,&#xA;&#xA;-- dimStore.dimStoreID&#xA;CASE&#xA;&#x9;WHEN dimStoreID IS NOT NULL THEN dimStore.dimStoreID&#xA;&#x9;ELSE (SELECT dimStoreID FROM dimStore&#xA;&#x9;&#x9;WHERE dimStore.StoreName = 'Unknown')&#xA;END AS dimStoreID,&#xA;&#xA;-- dimReseller.dimResellerID&#xA;CASE&#xA;&#x9;WHEN dimResellerID IS NOT NULL THEN dimReseller.dimResellerID&#xA;&#x9;ELSE (SELECT dimResellerID FROM dimReseller&#xA;&#x9;&#x9;WHERE dimReseller.ResellerName= 'Unknown')&#xA;END AS dimResellerID,&#xA;&#xA;-- dimCustomer.dimCustomerID&#xA;CASE&#xA;&#x9;WHEN dimCustomerID IS NOT NULL THEN dimCustomer.dimCustomerID&#xA;&#x9;ELSE (SELECT dimCustomerID FROM dimCustomer&#xA;&#x9;&#x9;WHERE dimCustomer.CustomerFullName= 'Unknown')&#xA;END AS dimCustomerID,&#xA;&#xA;&#xA;dimChannel.dimChannelID AS dimChannelID,&#xA;&#xA;-- dimSalesDateID&#xA;CASE&#xA;&#x9;WHEN StageSalesHeader.Date IS NOT NULL THEN (SELECT DimDate.DimDateID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM DimDate&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE StageSalesHeader.Date = DimDate.FullDate)&#xA;&#x9;ELSE NULL&#xA;END AS dimSalesDateID,&#xA;&#xA;-- dimLocationID&#xA;CASE&#xA;    WHEN dimStoreID IS NOT NULL THEN (SELECT dimLocationID &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;FROM dimLocation &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE LocationAddress = StageStore.Address &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationCity = StageStore.City &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationPostalCode = StageStore.PostalCode &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationState_Province = StageStore.StateProvince &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationCountry = StageStore.Country)&#xA;    WHEN dimResellerID IS NOT NULL THEN (SELECT dimLocationID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM dimLocation&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE LocationAddress = StageReseller.Address &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationCity = StageReseller.City &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationPostalCode = StageReseller.PostalCode &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationState_Province = StageReseller.StateProvince &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationCountry = StageReseller.Country)&#xA;&#x9;WHEN dimCustomerID IS NOT NULL THEN (SELECT dimLocationID &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM dimLocation&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE LocationAddress = StageCustomer.Address &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationCity = StageCustomer.City &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationPostalCode = StageCustomer.PostalCode &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationState_Province = StageCustomer.StateProvince &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND LocationCountry = StageCustomer.Country)&#xA;    ELSE NULL&#xA;END AS dimLocationID,&#xA;&#xA;StageSalesDetail.SalesHeaderID AS factSalesHeaderID,&#xA;StageSalesDetail.SalesDetailID AS factSalesDetailID,&#xA;StageSalesDetail.SalesAmount AS factSaleAmount,&#xA;StageSalesDetail.SalesQuantity AS factSaleQuantity,&#xA;ROUND(StageSalesDetail.SalesAmount/StageSalesDetail.SalesQuantity, 2) AS factSaleUnitPrice,&#xA;(dimProduct.ProductCost * StageSalesDetail.SalesQuantity) AS factSaleExtendedCost,&#xA;(StageSalesDetail.SalesAmount - (dimProduct.ProductCost * StageSalesDetail.SalesQuantity)) AS factSaleTotalProfit&#xA;&#xA;FROM&#xA;&#xA;StageSalesDetail&#xA;&#xA;LEFT JOIN StageSalesHeader ON StageSalesHeader.SalesHeaderID = StageSalesDetail.SalesHeaderID&#xA;LEFT JOIN StageReseller ON StageReseller.ResellerID = StageSalesHeader.ResellerID&#xA;LEFT JOIN dimReseller ON dimReseller.ResellerName = StageReseller.ResellerName&#xA;&#xA;LEFT JOIN StageStore ON StageStore.StoreID = StageSalesHeader.StoreID&#xA;LEFT JOIN dimStore ON  dimStore.StoreNumber = StageStore.StoreNumber&#xA;&#xA;LEFT JOIN StageChannel ON StageChannel.ChannelID = StageSalesHeader.ChannelID&#xA;LEFT JOIN dimChannel ON dimChannel.ChannelName = StageChannel.Channel&#xA;&#xA;LEFT JOIN StageCustomer ON StageCustomer.CustomerID = StageSalesHeader.CustomerID&#xA;LEFT JOIN dimCustomer ON dimCustomer.CustomerFullName = CONCAT(StageCustomer.FirstName, ' ', StageCustomer.LastName)&#xA;&#xA;LEFT JOIN StageProduct ON StageProduct.ProductID = StageSalesDetail.ProductID&#xA;LEFT JOIN dimProduct ON dimProduct.ProductName = StageProduct.Product&#xA;&#xA;&#xA;END&#xA;GO " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
